{
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "DocumentFragment": true,
        "define": true,
        "Meteor": true,
        "require": true,
        "exports": true,
        "module": true,
        "Template": true
    },
    "rules": {
        "valid-jsdoc": 0,
        "consistent-this": 2,
        "no-constant-condition": 1,
        "no-underscore-dangle": 0,
        "no-use-before-define": 1,
        "quotes": 0,
        "curly": 0,
        "func-names": 0,
        "new-parens": 2,
        "new-cap": 0,
        "no-ternary": 0,
        "no-unused-vars": [1, {"vars": "local", "args": "none"}],
        "one-var": 0,
        "space-infix-ops": 0,
        "strict": 1,
        "eqeqeq": 0,
        "dot-notation": 0
    }
}
